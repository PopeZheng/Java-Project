import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int x,n;
		x=sc.nextInt();
		n=sc.nextInt();
		sc.close();
		int i,j=0;
		for(i=1;i<=n;i++) {
			double a,b;
			a=(i+x)/7.0;
			b=(i+x-1)/7.0;
			if(a%1==0||b%1==0) {
				;
			}
			else {
				j++;
			}
		}
		System.out.println(250*j);
		
	}
}
